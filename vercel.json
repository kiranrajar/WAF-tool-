{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/index.js"
        },
        {
            "source": "/health",
            "destination": "/api/index.js"
        },
        {
            "source": "/dashboard",
            "destination": "/index.html"
        },
        {
            "source": "/dashboard/(.*)",
            "destination": "/$1"
        }
    ],
    "functions": {
        "api/index.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}